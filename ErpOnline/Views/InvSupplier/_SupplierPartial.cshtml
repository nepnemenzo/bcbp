@model IEnumerable<ErpOnline.Models.InvSupplierViewModel>

<table id="supplierTable" class="table table-bordered table-hover table-responsive table-striped" style="border:0px;">
    <thead>
        <tr>
            <th width="50">ID</th>
            <th>Name</th>
            <th>Agent</th>
            <th>Email</th>
            <th>Tell#</th>
            <th>Cell#</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
            {
            <tr data-id="@Html.DisplayFor(modelItem => item.SuppID)">
                <td>
                    @Html.DisplayFor(modelItem => item.SuppID)
                </td>
                <td>
                    @item.SuppName
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SuppAgent)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SuppEmailAdd)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SuppTelnum1)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SuppMobNum1)
                </td>
            </tr>
        }
    </tbody>
</table>