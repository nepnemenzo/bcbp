
@model IEnumerable<bcbp_101.Models.bcbp.CLPAttendanceModel>

<div>
    <table class="table table-striped table-bordered table-hover" id="CLPAttendanceDataTable">
        <thead>
            <tr>
                <th style="width:80px">
                    @Html.DisplayNameFor(model => model.participantId)
                </th>
                <th style="width:300px">
                    @Html.DisplayNameFor(model => model.id)
                </th>
                <th data-orderable="false" style="width:300px">
                    @Html.DisplayNameFor(model => model.participantName)
                </th>
                <th data-orderable="false" style="width:100px">
                    @Html.DisplayNameFor(model => model.talka)
                </th>
                <th data-orderable="false" style="width:100px">
                    @Html.DisplayNameFor(model => model.talkb)
                </th>
                <th data-orderable="false" style="width:100px">
                    @Html.DisplayNameFor(model => model.talkc)
                </th>
                <th data-orderable="false" style="width:100px">
                    @Html.DisplayNameFor(model => model.talkd)
                </th>
                <th data-orderable="false" style="width:100px">
                    @Html.DisplayNameFor(model => model.groupName)
                </th>
                <th data-orderable="false" style="width:100px">
                    @Html.DisplayNameFor(model => model.sponsor)
                </th>
            </tr>

        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr data-id="@Html.DisplayFor(modelItem => item.id)">
                    <td>
                        @Html.DisplayFor(modelItem => item.participantId)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.participantName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.talka)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.talkb)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.talkc)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.talkd)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.groupName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.sponsor)
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>


