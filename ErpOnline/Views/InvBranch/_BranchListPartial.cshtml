@model IEnumerable<ErpOnline.Models.Inventory.InvBranchModels>

<div>
    <table class="table table-striped table-bordered table-hover" id="branchDataTable">
        <thead>
            <tr>
                <th width="50" style="width:200px">
                    ID
                </th>
                <th>
                    Name
                </th>
                <th data-orderable="false" style="width:300px">
                    Address
                </th>
                <th data-orderable="false" style="width:300px">
                    Phone
                </th>
            </tr>

        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr data-id="@Html.DisplayFor(modelItem => item.branchID)">
                    <td>
                        @Html.DisplayFor(modelItem => item.branchID)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.address)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.phone)
                    </td>
  
                </tr>
            }
        </tbody>
    </table>
</div>

